{
  "manifest_version": 3,
  "name": "PageIntel by MaxTAF",
  "version": "1.1.0",
  "description": "PageIntel extracts, analyzes, and manages web content using customizable data tags and AI-driven tasks. \nEnhance your browsing experience by automating data collection, generating insightful analyses, and saving frequently used tasks for quick access. Perfect for researchers, analysts, and anyone who needs to streamline web data workflows.",
  "key": "",
  "icons": {
    "16": "icons/PageIntel_Brandmark_10 1.png",
    "48": "icons/PageIntel_Brandmark_10 1.png",
    "128": "icons/PageIntel_Brandmark_10 1.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "sidePanel"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "dist/panel.html"
  },
  "side_panel": {
    "default_path": "dist/panel.html"
  },
  "background": {
    "service_worker": "dist/background.bundle.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "dist/style.css"
      ],
      "js": [
        "dist/content.bundle.js"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "dist/content.bundle.js",
        "dist/api.js",
        "dist/generalTags.js",
        "dist/settings.js",
        "dist/tagManagement.js",
        "dist/taskManagement.js",
        "dist/panel.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "dist/style.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}